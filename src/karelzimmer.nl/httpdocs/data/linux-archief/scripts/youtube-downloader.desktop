[Desktop Entry]
Name=YouTube Downloader
Comment=Download YouTube video en/of audio
# Script getstarted past locatie /home/... aan.
Exec=/home/karel/scripts/youtube-downloader
# Script getstarted past locatie /home/... aan.
Icon=/home/karel/scripts/.icons/youtube-downloader.png
Type=Application
Actions=Bijwerken;

[Desktop Action Bijwerken]
Name=YouTube Downloader Bijwerken
# Script getstarted past locatie /home/... aan.
Exec=/home/karel/scripts/youtube-downloader --bijwerken

